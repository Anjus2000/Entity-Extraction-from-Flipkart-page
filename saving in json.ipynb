def extract_details(specifications):
    os = []
    hd = []
    sound = []
    for each in specifications:
        col = each.find_all('li', attrs={'class':'rgWa7D'})
        if len(col) >= 4:
            app = col[0].text
            os_ = col[1].text
            hd_ = col[2].text
            sound_ = col[3].text
            os.append(os_)
            hd.append(hd_)
            sound.append(sound_)
    return os, hd, sound
